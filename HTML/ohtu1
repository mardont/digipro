<!DOCTYPE HTML>
<html>
    <head>
    <meta charset="utf-8">
    <title>Digipro kysely</title>
    <link rel="stylesheet" type="text/css" href="tyylit.css">
    </head>
    
    <body>
        
    <form id="lomake"></form>
    
    <script
            src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js">
    </script>
    
    <script>
        $.getJSON("https://question-aika.herokuapp.com/questions/", function(data){
            var items =[]; //taulukko, johon kerätään lomake
            
         
            
            //jos hakasulku
            $.each(data.questions, function(key,d){
            items.push(d.question); 
            items.push("<br><input type=text id="+d.id+"><br><br>");
            });
            
            items.push("<input type=submit onclick=laheta>");
            // tulostus
            $("#lomake").append(items.join(" "));
        });
    </script>
    
    </body>
</html>
